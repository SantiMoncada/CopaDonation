{
  "name": "copadonation",
  "version": "1.0.0",
  "description": "Scripts for copadonation templates",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "./ServerExecutable release",
    "go:install": "go mod tidy",
    "build": "go build -o ServerExecutable src/*.go &&  webpack --config webpack.config.js",
    "dev": "concurrently --kill-others \"webpack --watch --config webpack.config.js\" \"nodemon --exec go run src/*.go --watch 'src/**/*' --signal SIGTERM\""
  },
  "author": "Santi",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "chart.js": "^4.4.0"
  }
}
